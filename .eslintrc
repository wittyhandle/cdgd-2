{
    "extends": [
        "react-app",
        "airbnb",
        "plugin:jsx-a11y/recommended",
        "prettier",
        "prettier/react"
    ],
    "plugins": [
        "jsx-a11y",
        "prettier"
    ],
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js"] }],
        "prettier/prettier": [
            2
        ],
        "react/jsx-props-no-spreading": [2,
            {
                "custom": "ignore"
            }
        ]
    }
}